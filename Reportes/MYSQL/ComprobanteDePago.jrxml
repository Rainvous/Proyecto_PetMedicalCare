	<query language="SQL"><![CDATA[SELECT 
    dp.DOCUMENTO_DE_PAGO_ID,
    dp.SERIE,
    dp.NUMERO,
    dp.FECHA_EMISION,
    dp.TIPO_DOCUMENTO,
    dp.ESTADO,
    dp.SUBTOTAL,
    dp.IGV_TOTAL,
    dp.TOTAL,
    mp.NOMBRE AS METODO_PAGO_NOMBRE,
    p.NOMBRE AS CLIENTE_NOMBRE,
    p.NRO_DOCUMENTO AS CLIENTE_DOCUMENTO
FROM DOCUMENTOS_DE_PAGO dp
LEFT JOIN METODOS_DE_PAGO mp
       ON dp.METODO_DE_PAGO_ID = mp.METODO_DE_PAGO_ID
LEFT JOIN PERSONAS p
       ON dp.PERSONA_ID = p.PERSONA_ID
WHERE dp.DOCUMENTO_DE_PAGO_ID = $P{P_DOCUMENTO_ID}
]]></query>
